# Neo4j Database Configuration
NEO4J_CONNECTION_URL=neo4j://127.0.0.1:7687
password=your_neo4j_password_here

# Google Gemini API
GOOGLE_API_KEY=your_google_api_key_here

# GitHub Access Token
